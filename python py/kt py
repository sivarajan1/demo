t=(1,2,3,4,5)
l=list(t)
l.append(66)
l.remove(3)
t=tuple(l)
print(t)


t = ( x for x in range(1,11) ifx%2==0:)
print(list(t))

s=set()
l=[1,2,3,4,5]
s.update(l)
print(s)
s.add(6)
print(s)

s={0,1,5,4,6,8,2,3,7}
s.pop()
