a=[("siva",23),("akhil",25),("kamal",23)]
a.sort

a=[("siva",23),("akhil",25),("kamal",23)]
b=sorted(a,key=lambda x:x[1])
print("sorted list")

for person in b:
    print(f"name:{person[0]},age:{person[1]}")

